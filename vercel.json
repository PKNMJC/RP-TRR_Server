{
  "version": 2,
  "buildCommand": "npm run build",
  "framework": "nestjs",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
